guarantee Addable {
  add(self: Self, other: Self) -> Self
}

guarantee Printable {
  print(self: Self) -> Unit
}

sig add(a: T, b: T) -> T
  require T guarantees Addable
  failure Never

@attr.io
@attr.handled
sig print(x: T) -> Unit
  require T guarantees Printable
  failure PrintErr

sig breath() -> Unit
  failure Never

