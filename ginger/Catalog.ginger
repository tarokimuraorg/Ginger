guarantee Addable {
  add(self: Self, other: Self) -> Self
}

guarantee Printable {
  print(self: Self) -> Unit
}

func add(a: T, b: T) -> T
  require T guarantees Addable
  failure Never

func print(x: T) -> Unit
  require T guarantees Printable
  failure PrintErr

func breath() -> Unit
  failure Never