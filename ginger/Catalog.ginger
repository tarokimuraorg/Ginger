// 保証
guarantee Addable {
  add(self: Self, other: Self) -> Self
}

// 型の集合（ドメイン）
typegroup Number = Int | Float
typegroup PrintErr = IOErr | EncodingErr


// 関数の仕様（足し算）
func add(a: T, b: T) -> T
  require T in Number
  require T guarantees Addable
  failure Never

// 値を表示する
func print(x: T) -> Unit
  require T guarantees Printable
  failure PrintErr